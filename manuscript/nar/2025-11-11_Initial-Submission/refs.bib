   issue = {10},
   journal = {Nature Methods},
   keywords = {Genomic analysis,Isolation,RNA,RNA folding,RNA metabolism,RNA sequencing,Retrovirus,Structure determination,binding proteins,separation and purification},
   month = {9},
   pages = {785-788},
