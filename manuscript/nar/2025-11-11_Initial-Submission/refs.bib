	isbn = {1740-1534},
	journal = {Nature Reviews Microbiology},
	number = {4},
	pages = {255--265},
	title = {Bacterial {RNA} thermometers: molecular zippers and switches},
	url = {https://doi.org/10.1038/nrmicro2730},
	volume = {10},
	year = {2012},
	bdsk-url-1 = {https://doi.org/10.1038/nrmicro2730}
}
@article{Serganov2013,
	abstract = {Riboswitches were discovered in 2002 in bacteria as RNA-based intracellular sensors of vitamin derivatives. During the last decade, naturally occurring RNA sensor elements have been found to bind a range of small metabolites and ions and to exert regulatory control of transcription, translation, splicing, and RNA stability. Extensive biochemical, structural, and genetic studies have established the basic principles underpinning riboswitch function in all three kingdoms of life with implications for developing antibiotics, designing new molecular sensors, and integrating riboswitches into synthetic circuits.},
	author = {Serganov, Alexander and Nudler, Evgeny},
	date = {2013/01/17},
