   keywords = {2,CoV,SARS,Translation},
   month = {8},
   pages = {5120},
   pmid = {34433827},
   publisher = {Nature Publishing Group},
   title = {A high-resolution temporal atlas of the {SARS-CoV-2} translatome and transcriptome},
   volume = {12},
   url = {https://www.nature.com/articles/s41467-021-25361-5},
   year = {2021},
}
