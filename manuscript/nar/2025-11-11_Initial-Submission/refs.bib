   doi = {10.1093/nar/gkv1189},
   journal = {Nucleic Acids Research},
   issue = {D1},
   keywords = {Animals,Cattle,Databases,Fungal,Gene Expression Profiling,Genetic*,Genome,Genomics* / standards,Human,Humans,Intramural,Invertebrates / genetics,Kim D Pruitt,Long Noncoding / genetics,MEDLINE,Mathew W Wright,Mice,Microbial,Molecular Sequence Annotation,N.I.H.,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Nematoda / genetics,Nuala A O'Leary,PMC4702849,Phylogeny,Plant,Protein,PubMed Abstract,RNA,Rats,Reference Standards,Research Support,Sequence Analysis,Vertebrates / genetics,Viral,doi:10.1093/nar/gkv1189,pmid:26553804},
   pages = {D733-D745},
